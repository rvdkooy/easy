    "compile:server": "tsc --project ./server",
    "compile:js": "webpack",
    "dev:server": "nodemon --exec \"./node_modules/.bin/ts-node\" -- ./server/server.ts --config ./server/nodemon.json",
    "dev:js": "webpack --watch",
    "dev": "concurrently --kill-others \"npm run dev:server\" \"npm run dev:js\"",
