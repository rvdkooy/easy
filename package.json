{
  "name": "easy",
  "version": "0.0.1",
  "description": "Easy Admin SYstem",
  "main": "index.js",
  "scripts": {
    "compile:server": "tsc --project ./server",
    "compile:js": "webpack --config ./webapp/webpack.config.js",
    "dev:server": "nodemon --exec \"./node_modules/.bin/ts-node --project server\" -- ./server/server.ts --config ./server/nodemon.json",
    "dev:js": "npm run compile:js -- --watch",
    "dev": "concurrently --kill-others \"npm run dev:server\" \"npm run dev:js\"",
    "debug:server": "npm run compile:server && npm run start",
    "postinstall": "npm run compile:server && npm run compile:js",
    "start": "node ./server/dist/server.js",
    "test:server": "TS_NODE_PROJECT=__tests__/server mocha -r ts-node/register --watch-extensions ts ./__tests__/**/*.spec.ts --timeout 120000 --exit",
    "test": "npm run test:server"
  },
